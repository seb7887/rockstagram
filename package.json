{
  "name": "rockstagram",
  "version": "1.0.0",
  "description": "Yet Another Instagram Clone",
  "main": "index.js",
  "scripts": {
    "build:webpack": "npm run webpack -- --env.mode production",
    "build": "npm run clean && npm run build:webpack",
    "clean": "rimraf dist",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "dev:client": "npm run webpack-dev-server -- --env.mode development --hot",
    "dev:server": "node server",
    "dev": "npm-run-all --parallel dev:*",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage --runInBand --detectOpenHandles --forceExit",
    "test": "NODE_ENV=test is-ci \"test:coverage\" \"test:watch\"",
    "start:server": "node server",
    "start:client": "serve --no-clipboard --single --listen 8080 dist",
    "start": "npm-run-all --parallel start:*",
    "format": "prettier \"**/*.+(js|json|less|css|ts|tsx|md)\" --write",
    "precommit": "npm run format",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seb7887/rockstagram.git"
  },
  "author": "Sebastian Matias Segura",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/seb7887/rockstagram/issues"
  },
  "homepage": "https://seb7887.github.io/rockstagram/",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "is-ci-cli": "^1.1.1",
    "jest": "^24.1.0",
    "jest-dom": "^3.1.2",
    "jest-watch-select-projects": "^0.1.1",
    "jest-watch-typeahead": "^0.2.1",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "react-testing-library": "^5.9.0",
    "rimraf": "^2.6.3",
    "serve": "^10.1.2",
    "start-server-and-test": "^1.7.11",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "dependencies": {
    "raven-js": "^3.27.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "react-transition-group": "^2.5.3",
    "redbox-react": "^1.6.0",
    "redux": "^4.0.1",
    "styled-components": "^4.1.3"
  }
}
